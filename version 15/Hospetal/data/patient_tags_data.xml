<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--    data noupdate="1" => the below field if i make on it any update from UI and update  Hospetal model the updates on the below will remove.. so to prevent that add records at data record with noupdate='1' attribute (136 NoUpdate Attribute In Odoo || ForceCreate Attribute In Odoo || Odoo Noupdate and Forcecreate)-->
    <data noupdate="1">
        <!--    dah XML file mazawed feh el data beta3t el tags (65)-->
        <record id="housewife_tag" model="patients.tags">
            <field name="name">Housewife</field>
        </record>
        <record id="ahmed_tag" model="patients.tags" forcecreate="0">
            <!--            forcecreate="0"=> 3shan lo mas7t ahmed tag from tags when upgrade will not created again -->
            <field name="name">Ahmed Tag</field>
        </record>
        <record id="eng_tag" model="patients.tags">
            <field name="name">Engineer</field>
            <field name="active" eval="False"/>
            <!--        3sha el active field is bollen to make it false by eval att
                         note:- lo 3`ayert el activity le true men el system we 5alet el field dah mesh archived lama t3ml update lel model hayrg3 archived-->
        </record>
    </data>
</odoo>
